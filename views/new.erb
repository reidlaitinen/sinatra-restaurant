<form method="POST" action="/items">
  <div class="form-group">
    <label for="itemName">Item Name</label>
    <input type="text" name="name" class="form-control" id="itemName" placeholder="" autocomplete='off'>
  </div>
  <div class="form-group">
    <label for="itemCourse">Course</label>
    <select class="form-control" id="itemCourse" name="course">
      <option>Drink</option>
      <option>Appetizer</option>
      <option>Side</option>
      <option>Entree</option>
      <option>Dessert</option>
    </select>
  </div>
  <div class="form-group">
    <label for="itemDesc">Description</label>
    <textarea class="form-control" id="itemDesc" name="desc" rows="3" autocomplete='off'></textarea>
  </div>
  <div class="form-group">
    <label for="itemPrice">Price</label>
    <input type="text" name="name" class="form-control" id="itemPrice" placeholder="00" autocomplete='off'>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>